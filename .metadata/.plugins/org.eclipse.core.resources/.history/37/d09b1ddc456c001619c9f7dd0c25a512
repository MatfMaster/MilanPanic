package milan.panic.master.weaving;

public class Program {
	public static void main(String[] args) {
		new Program();
		/**
		 * Aspektni savet smešta se pre poziva metoda, kao što je
		 * definisano jezikom
		 */
		ExampleAspect.aspectOf().beforeCall();
		method();
	}

	public static void method() {
		/**
		 * Aspektni savet pre izvršenja metoda(na početku izvršenja
		 * metoda)
		 */
		ExampleAspect.aspectOf().beforeExecution();
		System.out.println("method");
	}
}
